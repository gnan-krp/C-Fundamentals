#include<stdio.h>
void main(){
  int c=0,d=0,v=0,i;
  char str[50];

  printf("Enter A string = ");
  scanf("%s",str);

 for(i=0;str[i]!='\0';i++){
   if(str[i]>='0' && str[i]<='9')
     d++;
   else if(str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u' || str[i]=='A' ||
     str[i]=='E' || str[i]=='I' || str[i]=='O' || str[i]=='U'){
          v++;
     }
   else{
     c++;
   }
 }
   printf("Vowels = %d\nConsonants = %d\nDigits = %d", v, c, d);
}

#include<stdio.h>
void main(){
    char str[50],letters[50], digits[50], special[50];
    int i,l=0,d=0,s=0;
    
    printf("Enter A String = ");
    scanf("%s",str);
    
    for(i=0;str[i]!='\0';i++){
        if((str[i]>='A' && str[i]<='Z') || (str[i]>='a' && str[i]<='z')){
            letters[l++]=str[i];
        }
        else if(str[i]>='0' && str[i]<='9'){
            digits[d++]=str[i];
        }
        else{
            special[s++]=str[i];
        }
    }
    printf("Letters = %s\n",letters);
    printf("Digits = %s\n",digits);
    printf("Special Characters = %s\n",special);
    printf("Number Of Letters = %d\n",l);
    printf("Number Of Digits = %d\n",d);
    printf("Number Of Special Characters = %d\n",s);

}
